        $.ajax({
            url: '{{ path('rate_') }}',
            type: 'post',
            data: JSON.stringify(obj),
            success: function (result) {
                console.log(result);
                fillStars(result-1);
            }
        });
        console.log(number);
    })

    $(document).ready(function () {
        $(".mystar").each(function (i,e) {
            $(this).click(function () {

                fillStars(i);
            })
        })
        fillStars(Math.round({{ avis.rate-1 }}));
    });

    function fillStars(index) {
        $(".mystar").each(function (i,e) {
            if(i<=index){
                $(this).removeClass();
                $(this).addClass("fa fa-star mystar");
            }else{
                $(this).removeClass();
                $(this).addClass("fa fa-star-o mystar");
            }
        })
    }
</script>
